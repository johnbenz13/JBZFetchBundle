<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="jbz_fetch" class="JBZ\FetchBundle\Fetch\FetchWrapper">
            <argument>%jbz_fetch.host%</argument>
            <argument>%jbz_fetch.port%</argument>
            <argument>%jbz_fetch.username%</argument>
            <argument>%jbz_fetch.password%</argument>
            <argument>%jbz_fetch.service%</argument>
        </service>
    </services>

</container>
